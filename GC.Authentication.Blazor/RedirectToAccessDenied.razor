@inject NavigationManager NavigationManager

@code {
  [CascadingParameter]
  public AuthenticationOptions Options { get; set; } = new();
  protected override void OnInitialized()
  {
    NavigationManager.NavigateTo($"{Options.AccessDeniedPath}?returnUrl={Uri.EscapeDataString(NavigationManager.Uri)}",
    forceLoad: true);
  }
}
